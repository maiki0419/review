<div class="text-center mt-3">
  <%= form_with url: search_serches_path,local: true do |f| %>
    <%= f.text_field :word %>
    <%= f.select :range,[["User"],["Book"]] %>
    <%= f.select :match,[["完全一致","perfect"],["前方一致","forward"],["後方一致","backward"],["部分一致","part"]] %>
    <%= f.submit "検索" %>
  <% end %>
</div>

